<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="98dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">
    <LinearLayout
        android:background="@drawable/feed_item_border"
        android:id="@+id/feed_ll_schedules"
        android:layout_width="match_parent"
        android:layout_height="86dp"
        android:elevation="10dp"
        android:orientation="horizontal">
        <FrameLayout
            android:layout_width="12dp"
            android:layout_height="match_parent">
            <ImageView
                android:id="@+id/feed_iv_color_1"
                android:background="@drawable/feed_recycler_border_item_one"
                android:layout_width="12dp"
                android:layout_height="match_parent"/>
            <ImageView
                android:id="@+id/feed_iv_color_2"
                android:background="@drawable/feed_recycler_border_item_two"
                android:layout_width="12dp"
                android:layout_height="match_parent"/>
            <ImageView
                android:id="@+id/feed_iv_color_3"
                android:background="@drawable/feed_recycler_border_item_three"
                android:layout_width="12dp"
                android:layout_height="match_parent"/>
            <ImageView
                android:id="@+id/feed_iv_color_4"
                android:background="@drawable/feed_recycler_border_item_four"
                android:layout_width="12dp"
                android:layout_height="match_parent"/>
            <ImageView
                android:id="@+id/feed_iv_color_5"
                android:background="@drawable/feed_recycler_border_item_five"
                android:layout_width="12dp"
                android:layout_height="match_parent"/>
            <ImageView
                android:id="@+id/feed_iv_color_6"
                android:background="@drawable/feed_recycler_border_item_six"
                android:layout_width="12dp"
                android:layout_height="match_parent"/>
            <ImageView
                android:id="@+id/feed_iv_color_7"
                android:background="@drawable/feed_recycler_border_item_seven"
                android:layout_width="12dp"
                android:layout_height="match_parent"/>
            <ImageView
                android:id="@+id/feed_iv_color_8"
                android:background="@drawable/feed_recycler_border_item_eight"
                android:layout_width="12dp"
                android:layout_height="match_parent"/>

        </FrameLayout>
        <LinearLayout
            android:weightSum="86"
            android:layout_marginLeft="16dp"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_weight="38"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/feed_tv_title"
                    android:textSize="18dp"
                    android:fontFamily="@font/noto_sans_cjk_kr_medium"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:paddingTop="16dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_weight="48"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="horizontal">
                <TextView
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:alpha="0.7"
                    android:textColor="#000000"
                    android:textSize="14dp"
                    android:fontFamily="@font/notosanscjkr_regular"
                    android:id="@+id/feed_tv_today"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <LinearLayout
                    android:layout_marginRight="16dp"
                    android:gravity="center_vertical"
                    android:id="@+id/show_profile"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <FrameLayout
                        android:id="@+id/show_profile_frame"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:layout_gravity="end"
                            android:id="@+id/linear_two"
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <TextView
                                android:id="@+id/cnt_user_two"
                                android:text="2"
                                android:layout_gravity="center_vertical"
                                android:drawablePadding="4dp"
                                android:fontFamily="@font/notosanscjkr_regular"
                                android:textSize="12dp"
                                android:textColor="#99000000"
                                android:drawableStart="@drawable/ic_people_outline_24px"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="8dp"/>
                            <FrameLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">
                                <ImageView
                                    android:layout_gravity="end"
                                    android:layout_marginRight="16dp"
                                    android:id="@+id/two_person_first"
                                    android:layout_width="22dp"
                                    android:layout_height="22dp"/>
                                <ImageView
                                    android:layout_gravity="end"
                                    android:id="@+id/two_person_second"
                                    android:layout_width="22dp"
                                    android:layout_height="22dp"/>

                            </FrameLayout>

                        </LinearLayout>
                        <LinearLayout
                            android:layout_gravity="end"
                            android:id="@+id/linear_three"
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <TextView
                                android:id="@+id/cnt_user_three"
                                android:text="3"
                                android:layout_gravity="center_vertical"
                                android:drawablePadding="4dp"
                                android:fontFamily="@font/notosanscjkr_regular"
                                android:textSize="12dp"
                                android:textColor="#99000000"
                                android:drawableStart="@drawable/ic_people_outline_24px"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="8dp"/>
                            <FrameLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">
                                <ImageView
                                    android:layout_gravity="end"
                                    android:layout_marginRight="32dp"
                                    android:id="@+id/three_person_first"
                                    android:layout_width="22dp"
                                    android:layout_height="22dp"/>
                                <ImageView
                                    android:layout_gravity="end"
                                    android:layout_marginRight="16dp"
                                    android:id="@+id/three_person_second"
                                    android:layout_width="22dp"
                                    android:layout_height="22dp"/>
                                <ImageView
                                    android:layout_gravity="end"
                                    android:id="@+id/three_person_third"
                                    android:layout_width="22dp"
                                    android:layout_height="22dp"/>
                            </FrameLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_gravity="end"
                            android:id="@+id/linear_four"
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <TextView
                                android:id="@+id/cnt_user_four"
                                android:layout_gravity="center_vertical"
                                android:drawablePadding="4dp"
                                android:fontFamily="@font/notosanscjkr_regular"
                                android:textSize="12dp"
                                android:textColor="#99000000"
                                android:drawableStart="@drawable/ic_people_outline_24px"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="8dp"/>
                            <FrameLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">
                                <ImageView
                                    android:layout_marginRight="48dp"
                                    android:layout_gravity="end"
                                    android:id="@+id/four_person_first"
                                    android:layout_width="22dp"
                                    android:layout_height="22dp"/>
                                <ImageView
                                    android:layout_marginRight="32dp"
                                    android:layout_gravity="end"
                                    android:id="@+id/four_person_second"
                                    android:layout_width="22dp"
                                    android:layout_height="22dp"/>
                                <ImageView
                                    android:layout_marginRight="16dp"
                                    android:layout_gravity="end"
                                    android:id="@+id/four_person_third"
                                    android:layout_width="22dp"
                                    android:layout_height="22dp"/>
                                <ImageView
                                    android:layout_gravity="end"
                                    android:background="@drawable/ic_dot"
                                    android:id="@+id/four_person_fourth"
                                    android:layout_width="22dp"
                                    android:layout_height="22dp"/>
                            </FrameLayout>
                        </LinearLayout>

                    </FrameLayout>

                </LinearLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>

    </LinearLayout>
</LinearLayout>